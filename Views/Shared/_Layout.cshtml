<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>PYMORE - @ViewData["Title"]</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    @*<link href="~/assets/img/favicon.png" rel="icon">*@
    <link href="~/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true">
    <link rel="stylesheet" href="~/css/select2.min.css" asp-append-version="true" />
    <link href="~/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet" asp-append-version="true">
    <link href="~/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/assets/vendor/quill/quill.snow.css" rel="stylesheet" asp-append-version="true">
    <link href="~/assets/vendor/quill/quill.bubble.css" rel="stylesheet" asp-append-version="true">
    <link href="~/assets/vendor/remixicon/remixicon.css" rel="stylesheet" asp-append-version="true">
    <link href="~/assets/vendor/simple-datatables/style.css" rel="stylesheet" asp-append-version="true">

    <!-- Template Main CSS File -->
    <link href="~/assets/css/style.css" rel="stylesheet" asp-append-version="true">
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center">

        <div class="d-flex align-items-center justify-content-between">
            <a href="#" class="logo d-flex align-items-center"><img src="~/assets/img/logo.png" alt=""><span class="d-none d-lg-block">PYMORE</span></a>
            <i class="bi bi-list toggle-sidebar-btn"></i>
        </div><!-- End Logo -->

        <div class="search-bar">
            <form class="search-form d-flex align-items-center" method="POST" action="#">
                <input type="text" name="query" placeholder="Search" title="Enter search keyword">
                <button type="submit" title="Search"><i class="bi bi-search"></i></button>
            </form>
        </div><!-- End Search Bar -->

        <nav class="header-nav ms-auto">
            <ul class="d-flex align-items-center">

                <li class="nav-item d-block d-lg-none">
                    <a class="nav-link nav-icon search-bar-toggle " href="#">
                        <i class="bi bi-search"></i>
                    </a>
                </li><!-- End Search Icon-->

                <li class="nav-item dropdown pe-3">

                    <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">
                        <img src="~/assets/img/face15.jpg" alt="Profile" class="rounded-circle">
                        <span class="d-none d-md-block dropdown-toggle ps-2">M. Gathoni</span>
                    </a><!-- End Profile Iamge Icon -->

                    <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
                        <li class="dropdown-header">
                            <h6>Mburu Gathoni</h6>
                            <span>Web Designer</span>
                        </li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item d-flex align-items-center" href="#"><i class="bi bi-person"></i><span>My Profile</span></a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item d-flex align-items-center" href="#"><i class="bi bi-gear"></i><span>Account Settings</span></a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item d-flex align-items-center" href="#"><i class="bi bi-question-circle"></i><span>Need Help?</span></a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item d-flex align-items-center" href="#"><i class="bi bi-box-arrow-right"></i><span>Sign Out</span></a></li>

                    </ul><!-- End Profile Dropdown Items -->
                </li><!-- End Profile Nav -->

            </ul>
        </nav><!-- End Icons Navigation -->

    </header><!-- End Header -->
    <!-- ======= Sidebar ======= -->
    <aside id="sidebar" class="sidebar">

        <ul class="sidebar-nav" id="sidebar-nav">

            <li class="nav-item"><a class="nav-link collapsed" href="#"><i class="bi bi-grid"></i><span>Dashboard</span></a></li><!-- End Dashboard Nav -->

            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#tables-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-layout-text-window-reverse"></i><span>Inventory</span><i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="tables-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li><a asp-area="" asp-controller="Product" asp-action="Index"><i class="bi bi-circle"></i><span>Product</span></a></li>
                    <li><a asp-area="" asp-controller="Category" asp-action="Index"><i class="bi bi-circle"></i><span>Category</span></a></li>
                    <li><a asp-area="" asp-controller="Currency" asp-action="Index"><i class="bi bi-circle"></i><span>Currency</span></a></li>
                    <li><a asp-area="" asp-controller="Measure" asp-action="Index"><i class="bi bi-circle"></i><span>Measure Unit</span></a></li>
                    <li><a asp-area="" asp-controller="Discount" asp-action="Index"><i class="bi bi-circle"></i><span>Discount</span></a></li>
                </ul>
            </li><!-- End Tables Nav -->

            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#charts-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-bar-chart"></i><span>Sales</span><i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="charts-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li><a asp-area="" asp-controller="Customer" asp-action="Index"><i class="bi bi-circle"></i><span>Customers</span></a></li>
                    <li><a asp-area="" asp-controller="Invoice" asp-action="Index"><i class="bi bi-circle"></i><span>Sales Invoices</span></a></li>
                </ul>
            </li><!-- End Charts Nav -->

            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#icons-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-bar-chart"></i><span>Purchases</span><i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="icons-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li><a asp-area="" asp-controller="Supplier" asp-action="Index"><i class="bi bi-circle"></i><span>Suppliers</span></a></li>
                    <li><a asp-area="" asp-controller="HeaderInvoice" asp-action="Index"><i class="bi bi-circle"></i><span>Purchase Invoice</span></a></li>
                    <li><a asp-area="" asp-controller="GoodReceived" asp-action="Index"><i class="bi bi-circle"></i><span>Goods Received</span></a></li>
                </ul>
            </li><!-- End Icons Nav -->



        </ul>

    </aside><!-- End Sidebar-->

    <main id="main" class="main">

        <div class="pagetitle">
            <h1>Blank Page</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                    <li class="breadcrumb-item">Pages</li>
                    <li class="breadcrumb-item active">Blank</li>
                </ol>
            </nav>
        </div><!-- End Page Title -->

        <section class="section">
            <div class="row">
                <div class="col-lg-12">

                    <div class="card">
                        @RenderBody()
                    </div>

                </div>
            </div>
        </section>

    </main><!-- End #main -->
    <!-- ======= Footer ======= -->
    <footer id="footer" class="footer">
        <div class="copyright">
            &copy; Copyright <strong><span>NgenX</span></strong>. All Rights Reserved
        </div>
        <div class="credits">
            Designed by <a href="#">Mburu G.</a>
        </div>
    </footer><!-- End Footer -->

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/assets/vendor/apexcharts/apexcharts.min.js" asp-append-version="true"></script>
    <script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/select2.min.js" asp-append-version="true"></script>
    <script src="~/assets/vendor/chart.js/chart.umd.js" asp-append-version="true"></script>
    <script src="~/assets/vendor/echarts/echarts.min.js" asp-append-version="true"></script>
    <script src="~/assets/vendor/quill/quill.min.js" asp-append-version="true"></script>
    <script src="~/assets/vendor/simple-datatables/simple-datatables.js" asp-append-version="true"></script>
    <script src="~/assets/vendor/tinymce/tinymce.min.js" asp-append-version="true"></script>
    <script src="~/assets/vendor/php-email-form/validate.js" asp-append-version="true"></script>

    <!-- Template Main JS File -->
    <script src="~/assets/js/main.js" asp-append-version="true"></script>
    @await Component.InvokeAsync("Notyf");
    @await RenderSectionAsync("Scripts", required: false);
</body>

</html>